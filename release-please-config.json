{
  "packages": {
    ".": {
      "release-type": "rust",
      "component": "omen"
    },
    "plugin": {
      "release-type": "simple",
      "component": "omen-skills",
      "extra-files": [
        {
          "type": "json",
          "path": "plugin.json",
          "jsonpath": "$.version"
        }
      ]
    },
    ".claude-plugin": {
      "release-type": "simple",
      "component": "omen-marketplace",
      "skip-github-release": true,
      "extra-files": [
        {
          "type": "json",
          "path": "marketplace.json",
          "jsonpath": "$.metadata.version"
        }
      ]
    }
  }
}
